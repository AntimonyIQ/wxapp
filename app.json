{
  "expo": {
    "name": "wealthx",
    "slug": "wealthx",
    "version": "3.2.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "wealthx",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wealthx.wallet",
      "buildNumber": "25",
      "infoPlist": {
        "UIFileSharingEnabled": true,
        "LSSupportsOpeningDocumentsInPlace": true,
        "UIBackgroundModes": [
          "fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "permissions": [
        "INTERNET",
        "RECEIVE_BOOT_COMPLETED"
      ],
      "package": "com.wealthx.wallet"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-background-task",
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "64e78ea2-ceda-4ab0-b1ae-31f78ecbfe5e"
      }
    },
    "owner": "wealthx",
    "runtimeVersion": "1.0.0",
    "updates": {
      "enabled": false,
      "url": "https://u.expo.dev/64e78ea2-ceda-4ab0-b1ae-31f78ecbfe5e"
    }
  }
}
